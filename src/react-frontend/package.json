    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.11",
    "@types/styled-components": "5.1.25",
    "@types/testing-library__jest-dom": "5.14.2",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.4",
    "@uiw/react-md-editor": "3.12.3",
    "antd": "4.20.4",
    "axios": "0.27.2",
    "chalk": "5.0.1",
    "craco-antd": "2.0.0",
    "craco-less": "2.0.0",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "fontfaceobserver": "2.1.0",
    "http-proxy-middleware": "2.0.6",
    "i18next": "21.8.0",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-scanner": "3.1.0",
    "inquirer": "8.2.4",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.8",
    "jwt-decode": "3.1.2",
    "lint-staged": "12.4.1",
    "monaco-editor": "0.33.0",
    "node-plop": "0.31.0",
    "plop": "2.7.4",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.1.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.16.9",
    "react-is": "18.1.0",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "17.0.2",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "rehype-sanitize": "rehypejs/rehype-sanitize",
    "rimraf": "3.0.2",
    "serve": "13.0.2",
    "shelljs": "0.8.5",
    "styled-components": "5.3.5",
    "stylelint": "14.8.2",
    "stylelint-config-recommended-less": "1.0.4",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.4",
    "usehooks-ts": "2.5.2",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "nodemon -w craco.config.js -w ./src/app/styles/theme.less --exec \"craco start\"",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "nodemon": "2.0.16",
    "sass-loader": "12.6.0",
    "stylelint-config-recommended": "7.0.0"
  }
}
